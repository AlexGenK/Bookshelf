<%= form_for book, html: { multipart: true } do |f| %>
	<%= f.label :cover %><br>
	<%= image_tag book.cover.url(:medium) %><br>
	<%= f.file_field :cover %><br>
	<%= f.label :title %>
	<%= f.text_field :title %><br>
	<%= f.label :description %>
	<%= f.text_field :description %><br>
	<%= f.submit %>
<% end %>